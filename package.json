  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.16.0",
    "@mui/material": "^5.16.0",
    "@mui/system": "^5.16.0",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.1",
    "redux-persist": "^6.0.0",
    "swiper": "^11.1.4"
  },
  "devDependencies": {
    "@mate-academy/eslint-config": "^0.0.29",
    "@reduxjs/toolkit": "^2.2.6",
    "@types/axios": "^0.14.0",
