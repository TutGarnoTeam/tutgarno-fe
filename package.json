    "sass": "^1.77.6",
    "typescript": "^5.5.3",
    "vite": "^5.3.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
